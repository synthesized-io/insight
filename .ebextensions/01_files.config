files:
    "/etc/nginx/conf.d/proxy.conf":
        mode: "000644"
        owner: root
        group: root
        content: |
            client_max_body_size 100M;

    "/etc/nginx/conf.d/synthesized.conf":
        mode: "000644"
        owner: root
        group: root
        content: |
            server {
                listen       80;
                server_name  _;
                if ($http_x_forwarded_proto != 'https') {
                    return 301 https://$host$request_uri;
                }
            }